{
    "name": "swishdigital/out-of-stock",
    "description": "Get notified when products are (almost) out of stock.",
    "type": "craft-plugin",
    "version": "3.0.3.1",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "out of stock"
    ],
    "support": {
        "docs": "https://github.com/chadcrowell/out-of-stock/blob/master/README.md",
        "issues": "https://github.com/chadcrowell/out-of-stock/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Swish Digital",
            "homepage": "https://swishdigital.co"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0",
        "craftcms/commerce": ">=2.1.0"
    },
    "autoload": {
        "psr-4": {
          "swishdigital\\outofstock\\": "src/"
        }
    },
    "extra": {
        "name": "Out of Stock",
        "handle": "out-of-stock",
        "developer": "Swish Digital",
        "developerUrl": "https://swishdigital.co",
        "documentationUrl": "https://github.com/chadcrowell/out-of-stock/blob/master/README.md",
        "changelogUrl": "https://raw.githubusercontent.com/chadcrowell/out-of-stock/master/CHANGELOG.md",
        "components": {
            "outOfStockService": "swishdigital\\outofstock\\services\\OutOfStockService"
        },
        "class": "swishdigital\\outofstock\\OutOfStock"
    }
}
